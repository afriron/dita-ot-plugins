<highlighters>
 <highlighter type="multiline-comment">
    <start>&amp;lt;!--</start>
    <end>--&amp;gt;</end>
  </highlighter>
   <highlighter type="oneline-comment">
    <start>&amp;lt;!--</start>
    <end>--&amp;gt;</end>
  </highlighter>
   <highlighter type="oneline-comment">
    <start>&lt;!--</start>
    <end>--&gt;</end>
  </highlighter>
   <highlighter type="multiline-comment">
    <start>&lt;!--</start>
    <end>--&gt;</end>
  </highlighter>
  <highlighter type='xml'>
  <elementPrefix>
    <style>xslt</style>
    <prefix>xsl:</prefix>
  </elementPrefix>
</highlighter>
</highlighters>